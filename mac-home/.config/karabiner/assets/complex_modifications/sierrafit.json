        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
              "left_option"
              ],
              "optional": [
                "any"
              ]

            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["right_command"]
            }
          ]
        }
      ]
    },
