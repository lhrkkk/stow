{
  "title": "my sierra fit new",
  "rules": [
    {
      "description": "Cycle: Left command, option, control swap Cycle,delete to backslash",
      "manipulators": [

        {
          "type": "basic",
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "optional": [
                "caps_lock",
                "shift",
                "command",
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "backslash"
            }
          ]
        },

        {
          "type": "basic",
          "from": {
            "key_code": "backslash",
            "modifiers": {
              "optional": [
                "caps_lock",
                "shift",
                "command",
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ]
        },    
        
        
        
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_option"
            }
          ]    
        },
        {
          "type": "basic",
          "from": {
            "key_code": "left_option",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_control"
            }
          ]
        },   



        {
          "type": "basic",
          "from": {
            "key_code": "left_control",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^com.googlecode.iterm2"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Tab: Left option + tab, ` to command + tab,` ` ",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": {
              "mandatory": [
              "left_option"
              ],
              "optional": [
                "caps_lock",
                "shift",
                "command",
                "option"
              ]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde",
              "modifiers": ["right_command"]
