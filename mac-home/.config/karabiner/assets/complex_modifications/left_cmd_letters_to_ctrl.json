{
  "title": "Left Command letters â†’ Control letters",
  "rules": [
    {
      "description": "Map left_command + w/t/r/i/n/p/b/c/q to control + same letter",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": ["left_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "w",
              "modifiers": ["control"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com.googlecode.iterm2",
                "org.alacritty",
                "com.github.wez.wezterm",
                "dev.warp.Warp-Stable"
              ]
            }
          ]
        },
